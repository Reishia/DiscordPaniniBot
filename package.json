{
  "name": "botdiscordpanini",
  "version": "0.1.0",
  "description": "Projet de bot Discord de collection de cartes Panini",
  "main": "main.js",
  "repository": "https://github.com/Reishia/DiscordPaniniBot.git",
  "author": "Reishia <alrerfr@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "discordjs/discord.js"
  },
  "devDependencies": {
    "eslint": "^6.2.2",
    "eslint-config-aqua": "^6.0.0"
  },
  "eslintConfig": {
    "extends": "aqua/node",
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "curly": "off",
      "no-trailing-spaces": "off",
      "prefer-template": "off",
      "no-eval": "off"
    }
  }
}
